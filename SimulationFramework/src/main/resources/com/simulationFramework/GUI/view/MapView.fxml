<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.web.WebView?>

<BorderPane prefHeight="600.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.simulationFramework.GUI.controller.MapController">
   <center>
      <WebView fx:id="wvMap" minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" BorderPane.alignment="CENTER" />
   </center>
   <top>
      <ButtonBar buttonMinWidth="10.0" style="-fx-background-color: white;" BorderPane.alignment="CENTER">
        <buttons>
          <Button minWidth="8.0" mnemonicParsing="false" onAction="#butCloseAction" prefHeight="25.0" prefWidth="26.0" text="X" />
        </buttons>
      </ButtonBar>
   </top>
</BorderPane>
